ate(false)\n\n  useEffect(() => {\n    const decodedHash = decodeURI(location.hash)\n    const currentHash = decodedHash && decodedHash.slice(1, Infinity)\n    setActive(Boolean(slug === currentHash))\n  }, [location])\n\n  return <Link as={Component} {...props} className={isActive ? 'active' : ''} />\n}\n"]}]}