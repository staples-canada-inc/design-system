.</Spinner>\\n<Spinner color=\\\"secondary\\\" size=\\\"lg\\\">\\n  Loading...\\n</Spinner>'} __scope={{\n      props: this ? this.props : props,\n      Playground,\n      Props,\n      SpinnerStaples,\n      Spinner\n    }} mdxType=\"Playground\">\n\t<Spinner color=\"secondary\" size=\"sm\" mdxType=\"Spinner\">\n\t\tLoading...\n\t</Spinner>\n\t<Spinner color=\"secondary\" mdxType=\"Spinner\">Loading...</Spinner>\n\t<Spinner color=\"secondary\" size=\"lg\" mdxType=\"Spinner\">\n\t\tLoading...\n\t</Spinner>\n    </Playground>\n    <pre><code parentName=\"pre\" {...{}}>{`<div class=\"spinner-border text-secondary\" role=\"status\">\n  <span class=\"sr-only\">Loading...</span>\n</div>\n`}</code></pre>\n    <h3 {...{\n      \"id\": \"assistive-technology\"\n    }}>{`Assistive Technology`}</h3>\n    <p>{`For accessibility purposes, each loader here includes `}<inlineCode parentName=\"p\">{`role=\"status\"`}</inlineCode>{` and a nested `}<inlineCode parentName=\"p\">{`<span class=\"sr-only\">Loading...</span>`}</inlineCode>{`.`}</p>\n    </MDXLayout>;\n}\n\nMDXContent.isMDXComponent = true;\n  "]}]}