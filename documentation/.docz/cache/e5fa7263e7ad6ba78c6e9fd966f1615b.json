eturn headings && headings.length > 0 ? (\n\t\t<Location>\n\t\t\t{({ location }) => (\n\t\t\t\t<Submenu>\n\t\t\t\t\t{headings.map((heading: any) => (\n\t\t\t\t\t\t<SmallLink\n\t\t\t\t\t\t\tas={Link}\n\t\t\t\t\t\t\tlocation={location}\n\t\t\t\t\t\t\tkey={heading.slug}\n\t\t\t\t\t\t\tonClick={onClick}\n\t\t\t\t\t\t\tto={`${route}#${heading.slug}`}\n\t\t\t\t\t\t\tslug={heading.slug}\n\t\t\t\t\t\t>\n\t\t\t\t\t\t\t{heading.value}\n\t\t\t\t\t\t</SmallLink>\n\t\t\t\t\t))}\n\t\t\t\t</Submenu>\n\t\t\t)}\n\t\t</Location>\n\t) : null;\n};\n"]}]}