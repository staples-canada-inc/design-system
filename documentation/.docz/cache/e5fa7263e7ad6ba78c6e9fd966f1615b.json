eadings.length > 0 ? (\n    <Location>\n      {({ location }) => (\n        <Submenu>\n          {headings.map((heading: any) => (\n            <SmallLink\n              as={Link}\n              location={location}\n              key={heading.slug}\n              onClick={onClick}\n              to={`${route}#${heading.slug}`}\n              slug={heading.slug}\n            >\n              {heading.value}\n            </SmallLink>\n          ))}\n        </Submenu>\n      )}\n    </Location>\n  ) : null\n}\n"]}]}