t: \"\";\n\tdisplay: block;\n\tposition: absolute;\n\twidth: 100%;\n\theight: 2px;\n\tleft: 0;\n\tright: 0;\n\tbackground: #ffffff;\n    transition: transform 0.3s, opacity 0.3s;\n    outline: none !important;\n\n\t&:nth-of-type(1) {\n\t\ttop: -2px;\n\t\ttransform: translateY(${IconFirst}) rotate(${IconRotate});\n\t}\n\n\t&:nth-of-type(2) {\n\t\ttop: 6px;\n\t\topacity: ${IconMiddle};\n\t}\n\n\t&:nth-of-type(3) {\n\t\ttop: 14px;\n\t\ttransform: translateY(${IconLast}) rotate(-${IconRotate});\n\t}\n`;\n\nconst ToggleButton = styled.button<OpenProps>`\n    cursor: pointer;\n    position: relative;\n    display: flex;\n    align-items: center;\n    justify-content: center;\n    padding: 5px 6px;\n    margin-right: 15px;\n\twidth: 35px;\n\theight: 25px;\n    z-index: 1031;\n    transition: all 0.3s;\n    outline: none !important;\n    border: none;\n    background: transparent;\n    &:hover{\n        opacity: .6;\n    }\n\n  ${mq({\n\t\tdisplay: [\"block\", \"block\", \"block\", \"block\"]\n  })};\n`;\n\ninterface HamburgerProps extends OpenProps {\n\tonClick: (ev: React.SyntheticEvent<any>) => void;\n}\n\nexport const Hamburger: SFC<HamburgerProps> = ({ opened, onClick }) => (\n\t<ToggleButton opened={opened} onClick={onClick}>\n\t\t<Icon opened={opened}>\n\t\t\t<IconLine opened={opened} />\n\t\t\t<IconLine opened={opened} />\n\t\t\t<IconLine opened={opened} />\n\t\t</Icon>\n\t</ToggleButton>\n);\n"]}]}